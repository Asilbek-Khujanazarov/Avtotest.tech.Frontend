{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist/out-tsc-server",
    "baseUrl": "./",
    "module": "commonjs",
    "types": [
      "node"
    ],
    "paths": {
      "@/*": ["src/app/*"]
    }
  },
  "files": [
    "src/main.server.ts",
    "src/app/app.server.module.ts"
  ],
  "angularCompilerOptions": {
    "entryModule": "src/app/app.server.module#AppServerModule"
  }
}